<html>
<h1>Out In Space</h1>
<br>

<form id="choose-station" action="/container" method="get">
 <label>Station:</label> <select name="id"> 
        {{#each station}}
        <option value="{{id}}">{{name}}</option>
        {{/each}}
    </select><br>
</form>
<label></label><button onclick="selectContainer({{id}})">Dock</button><br>
<br/>


<form id="choose-location" action="/location" method="get">
    <label>Location:</label> <select name="id"> 
        {{#each location}}
        <option value="{{id}}">{{name}}</option>
        {{/each}}
    </select><br>
</form>
<label></label><button onclick="selectLocation({{id}})">Move</button><br>

<br/>

<form id="choose-item" action="/item" method="get">
    <label>Item:</label> <select name="id"> 
        {{#each item}}
        <option value="{{id}}">{{name}}</option>
        {{/each}}
    </select><br>
</form>

<label></label><button onclick="deleteItem({{id}})">Jettison</button><br>
 

<h3>Chart A Wormhole</h3>
<form id="wormhole" action="/location" method="get">

<label>Wormhole Name:</label> <input type="text" name="name" value="name"><br>
<label>Wormhole Security Status:</label> <input type="text" name="Security" value="-10.0 - 10.0"><br>

Wormhole Name: <input type="text" name="name" value="System Name"><br>
Wormhole Security Status: <input type="number" step="0.1" min="-10.0" max="10.0" name="Security" value="10.0"><br>


<label></label><input type="submit" value="Invent">
</form>
</html>
